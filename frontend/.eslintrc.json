{
  "extends": [
    "airbnb",
    "react-app",
    "react-app/jest",
    "prettier"
  ],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "no-console": "off",
    "no-alert": "off",
    "react/jsx-props-no-spreading": ["off"],
    "react/require-default-props": [0],
    "react/prop-types": "off"
  }
}
